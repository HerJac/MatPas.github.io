			function pakeiskž01()
			{
			if (document.getElementById("v1").innerHTML=="+")
					document.getElementById("v1").innerHTML="-";
				else if (document.getElementById("v1").innerHTML=="-")
					document.getElementById("v1").innerHTML="x";
				else if (document.getElementById("v1").innerHTML=="x")
					document.getElementById("v1").innerHTML="/";
				else if (document.getElementById("v1").innerHTML=="/")
					document.getElementById("v1").innerHTML="+";
			}
			function pakeiskž02()
			{
