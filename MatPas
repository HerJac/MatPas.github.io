			{
			if (document.getElementById("v1").innerHTML=="x" && document.getElementById("v2").innerHTML=="x" && document.getElementById("v3").innerHTML=="x" && document.getElementById("v4").innerHTML=="x" && document.getElementById("ats1").innerHTML=="-")
			 document.getElementById("ats1").innerHTML="+";
			else if (document.getElementById("v1").innerHTML=="+" && document.getElementById("v2").innerHTML=="+" && document.getElementById("v3").innerHTML=="x" && document.getElementById("v4").innerHTML=="x" && document.getElementById("ats1").innerHTML=="-")
			 document.getElementById("ats1").innerHTML="+";
			else alert("Bandyk dar kartą");
			}
